{
  "name": "dockjump",
  "description": "Docker-based PostgreSQL developer environment with Graphile Migrate",
  "keywords": [
    "postgresql",
    "postgres",
    "pg",
    "migrate",
    "graphile-migrate",
    "postgraphile"
  ],
  "version": "0.1.2",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "bin": "dist/cli.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "del dist/",
    "build": "tsc",
    "postbuild": "cp src/*.sh dist && mkdir dist/generated && cp src/generated/*.json dist/generated && chmod +x dist/cli.js && shebang-trim dist/cli.js",
    "predev": "del dist/",
    "dev": "tsc --watch",
    "check-types": "tsc --noEmit --project .",
    "lint": "eslint \"**/*.ts\"",
    "prettier:check": "prettier --check \"**/*.@(ts|js|yml|md|json)\"",
    "prettier": "prettier --write \"**/*.@(ts|js|yml|md|json)\"",
    "schema:update": "1schema update",
    "test": "mocha",
    "prepare": "npm run build"
  },
  "files": [
    "dist/**/*.@(js|json|ts|map|sh)"
  ],
  "repository": "metabolize/dockjump",
  "dependencies": {
    "ajv": "^8.12.0",
    "argparse": "^2.0.1",
    "child-process-promise": "^2.2.1",
    "cosmiconfig": "^8.1.3",
    "lodash": "^4.17.21",
    "type-fest": "3.7.1",
    "wait-promise": "^0.4.1"
  },
  "peerDependencies": {
    "graphile-migrate": "*",
    "pg": "*"
  },
  "devDependencies": {
    "@types/argparse": "2.0.10",
    "@types/chai": "4.3.4",
    "@types/child-process-promise": "2.2.2",
    "@types/dirty-chai": "2.0.2",
    "@types/lodash": "4.14.192",
    "@types/mocha": "10.0.1",
    "@types/node": "18.15.10",
    "@types/wait-promise": "0.4.0",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "@typescript-eslint/parser": "5.57.0",
    "1schema": "0.2.0",
    "chai": "4.3.7",
    "chai-as-promised": "7.1.1",
    "del-cli": "5.0.0",
    "dirty-chai": "2.0.1",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.6.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "mocha": "10.2.0",
    "prettier": "2.8.7",
    "shebang-trim": "1.2.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.2"
  },
  "engines": {
    "node": ">= 16.x"
  },
  "mocha": {
    "extension": [
      "ts"
    ],
    "spec": "src/**/*.spec.ts",
    "loader": "ts-node/esm"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true,
    "arrowParens": "avoid"
  }
}
